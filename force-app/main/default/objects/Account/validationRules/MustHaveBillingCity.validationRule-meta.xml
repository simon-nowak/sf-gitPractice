<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MustHaveBillingCity</fullName>
    <active>true</active>
    <description>Account must have a billing city</description>
    <errorConditionFormula>ISBLANK(BillingCity)</errorConditionFormula>
    <errorMessage>City can not be empty</errorMessage>
</ValidationRule>
